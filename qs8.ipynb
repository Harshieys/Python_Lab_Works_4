    "def sumOfSeries(num):\n",
    "    res = 0\n",
    "    fact = 1\n",
    "    for i in range(1, num+1):\n",
    "        fact *= i\n",
    "        res = res + (i**i/ fact)\n",
    "        print(f\"{i**i}/{i}!+\")\n",
    "    return res\n",
    "n=int(input(\"Enter number:\"))\n",
    "print(\"Sum: \", sumOfSeries(n))"
   ]
